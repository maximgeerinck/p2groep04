<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane minHeight="400.0" minWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ViewEntityController">
<center><GridPane fx:id="gpView" hgap="10.0" vgap="10.0" BorderPane.alignment="CENTER">
  <columnConstraints>
    <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="300.0" minWidth="10.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
<BorderPane.margin>
<Insets />
</BorderPane.margin>
<padding>
<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
</padding>
</GridPane>
</center>
<bottom><ToolBar prefWidth="200.0" BorderPane.alignment="CENTER">
  <items>
    <Button fx:id="btnSave" mnemonicParsing="false" text="Save" /><Button fx:id="btnCancel" mnemonicParsing="false" onAction="#btnCancelHandle" text="Cancel" />
  </items>
</ToolBar>
</bottom>
<top><Pane prefHeight="65.0" prefWidth="600.0" BorderPane.alignment="CENTER">
<children><Label fx:id="lblTitle" layoutX="14.0" layoutY="17.0" text="Label">
<font>
<Font size="22.0" />
</font></Label>
</children></Pane>
</top>
</BorderPane>
